      </div>
      <% if(blog.length) { %>
      <div class="jumbotron jumbotron-fluid" style="width:60%; height:500px; overflow-y:auto">
        <div class="accordion" id="accordionExample" style="width: 60%; overflow-y: auto; margin:auto">
          <% for(let i=blog.length-1;i>=0;i--) { %>
          <div class="card">
            <div class="card-header" id="headingFour">
              <h2 class="mb-0">
                <button class="btn btn-link btn-block text-left collapsed" type="button" data-toggle="collapse" data-target="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
                  <%= blog[i].title %>
                </button>
              </h2>
            </div>
            <div id="collapseFour" class="collapse" aria-labelledby="headingFour" data-parent="#accordionExample">
              <div class="card-body">
                <h3>Date:- </h3><%= blog[i].date %>
                <h3>Comment:- </h3><%=blog[i].comment.length %>
                <h3>Like:- </h3><%=blog[i].like %>
              </div>
            </div>
          </div>
          <% } %>
        </div>
      </div>
      <% } else { %>
        <div class="jumbotron jumbotron-fluid" style="width:60%; height:500px; overflow-y:auto">
          <h1>No Blog Posted Yet!</h1>
